    "configurations": [
        {
            "name": "[x64|Windows]: Run and Debug (Proxy)",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/bin/proxy.exe",
            
            "MIMode": "gdb",
            "miDebuggerPath": "C:/mingw64/bin/gdb.exe",
